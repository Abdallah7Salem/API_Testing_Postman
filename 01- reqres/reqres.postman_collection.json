					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is correct\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"per_page\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.per_page).to.eql(6);\r",
							"})\r",
							"\r",
							"pm.test(\"first ID is 7\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.data[0].id).to.eql(7);\r",
							"})\r",
							"\r",
							"pm.test(\"id : 9 avatar \", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.data[2].avatar).to.eql(\"https://reqres.in/img/faces/9-image.jpg\");\r",
							"})\r",
							"\r",
							""
